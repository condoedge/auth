{
    "name": "condoedge/auth",
    "description": "Authentication and teams to your kompo application",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Condoedge\\Auth\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": ">=5.6.0",
        "kompo/kompo": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Condoedge\\Auth\\Providers\\KompoAuthServiceProvider"
            ]
        }
    },
    "prefer-stable": true
}
